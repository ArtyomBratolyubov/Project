@model MvcPL.Models.SingersViewModel

@{
    ViewBag.Title = "Singers";
    ViewBag.UserID = Model.UserId;
    ViewBag.Roles = Model.Roles;
}


@section main{
    <div class="panel ">

        <h3>
            Singers
            <img src="~/Content/images/loader.gif" class="load-img" id="loadingIndicator">
            @Html.ActionLink("Add", "Create", "Singer", null, new { @class = "btn btn-danger pull-right" })
        </h3>

    </div>
    <div class="panel-group">
        <div id="accordion">
            @Html.Partial("Page", Model.Singers)
        </div>
    </div>
    @Html.Partial("_paginator")

}

