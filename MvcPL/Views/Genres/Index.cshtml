@model MvcPL.Models.GenresViewModel

@{
    ViewBag.Title = "Genres";
    ViewBag.UserID = Model.UserId;
    ViewBag.Roles = Model.Roles;
}


@section main{
    <div class="panel ">

        <h3>
            Genres
            <img src="~/Content/images/loader.gif" class="load-img" id="loadingIndicator">
            @Html.ActionLink("Add", "Create","Genre", null, new { @class = "btn btn-danger pull-right" })
        </h3>

    </div>
    <div class="panel-group" id="accordion">
        @Html.Partial("Page", Model.Genres)
    </div>

}


